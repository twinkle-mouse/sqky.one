---
import type { HTMLAttributes } from "astro/types";

export type Props = HTMLAttributes<"img">;

const props = Astro.props;
---

<img {...props} alt="" loading="eager" />

<style>
    img {
        display: inline-block;
        width: 1.25em;
        height: 1.25em;
        vertical-align: text-bottom;
    }
</style>
